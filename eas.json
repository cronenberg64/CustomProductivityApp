{
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENABLE_FIREBASE": "false"
      }
    },
    "production": {
      "android": {
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "ENABLE_FIREBASE": "true"
      }
    }
  }
}